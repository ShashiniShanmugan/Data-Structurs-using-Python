        "#array special product without division\n",
        "import numpy as np\n",
        "x = np.array([1,2,3,4,5])\n",
        "def pro_ExceptSelf(x):\n",
        "   n = len(x)\n",
        "   res = [1]*n\n",
        "   for i in range(n):\n",
        "     for j in range(n):\n",
        "      if i != j:\n",
        "        res[i] *= x[j]\n",
        "   return res\n",
        "\n",
        "res = pro_ExceptSelf(x)\n",
        "print(res)\n"
      ]
    }
  ]
