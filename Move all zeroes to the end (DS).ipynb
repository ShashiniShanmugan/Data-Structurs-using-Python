        "import numpy as np\n",
        "x = np.array([0,1,1,0,1,0,1,0,0])\n",
        "def last1_occur(x):\n",
        "  store = 0\n",
        "  count = 0\n",
        "  n = len(x)\n",
        "  for i in x:\n",
        "    if i == 1:\n",
        "      store += i\n",
        "    else:\n",
        "      count += 1\n",
        "  return ( [1]*store)+([0]*count)\n",
        "print(last1_occur(x))"
